{
  "name": "@notificationapi/react",
  "private": false,
  "version": "1.9.0",
  "type": "module",
  "overrides": {
    "esbuild": "^0.25.0",
    "vue-template-compiler": "^2.7.16"
  },
  "browserslist": [
    "last 2 versions",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "not IE >= 1",
    "not bb >=1",
    "not IE_Mob >=1",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"{src,lib}/**/*.{ts,tsx,js,jsx,json,css,md,html}\"",
    "prettier-check": "prettier --check \"{src,lib}/**/*.{ts,tsx,js,jsx,json,css,md,html}\"",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.2 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.12.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-compat": "^6.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "faker-js": "^1.0.0",
    "glob": "^11.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/notificationapi-com/notificationapi-react-client-sdk"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@notificationapi/core": "^1.1.0",
    "javascript-time-ago": "^2.5.10",
    "liquidjs": "^10.14.0",
    "rc-virtual-list": "^3.11.5",
    "react-time-ago": "^7.3.3"
  }
}
